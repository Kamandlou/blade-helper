{
  "name": "kamandlou/blade-helper",
  "description": "Laravel Blade Helper",
  "keywords": [
    "blade-helper",
    "blade",
    "helper",
    "laravel"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/kamandlou/blade-helper/issues",
    "source": "https://github.com/kamandlou/blade-helper"
  },
  "autoload": {
    "psr-4": {
      "Kamandlou\\BladeHelper\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Kamandlou",
      "email": "ahkamandlou@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "illuminate/support": "^9.11"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kamandlou\\BladeHelper\\BladeHelperServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
